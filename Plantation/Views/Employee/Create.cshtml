            format: 'mm/dd/yyyy',
            autoclose: true
        }).datepicker("setDate", new Date());

        $('#DATEJOIN').datepicker({
            format: 'mm/dd/yyyy',
            autoclose: true
        }).datepicker("setDate", new Date());

        var message = '@message';
        if (message) {
            bootbox.alert(message);
        }
    </script>
    <script>
        $("#SEX").select2({ allowClear: true });
        $("#RELIGION").select2({ allowClear: true });
        $("#RACE").select2({ allowClear: true });
